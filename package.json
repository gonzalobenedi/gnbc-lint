{
  "name": "gnbc-lint",
  "version": "2.0.1",
  "description": "ESLint configurations for React, Angular, React Native, and Node.js projects with TypeScript and Prettier integration",
  "keywords": [
    "eslint",
    "eslint-config",
    "angular",
    "react",
    "react-native",
    "nodejs",
    "typescript",
    "prettier",
    "formatting",
    "linting",
    "code-style"
  ],
  "license": "MIT",
  "author": {
    "name": "Gonzalo Benedi",
    "email": "your-email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gonzalobenedi/gnbc-lint.git"
  },
  "bugs": {
    "url": "https://github.com/gonzalobenedi/gnbc-lint/issues"
  },
  "homepage": "https://github.com/gonzalobenedi/gnbc-lint#readme",
  "main": "index.js",
  "files": [
    "index.js",
    "react.js",
    "angular.js",
    "react-native.js",
    "node.js",
    "typescript.js",
    ".prettierrc",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "node test-configs.js",
    "lint": "eslint *.js",
    "prepare": "husky",
    "commitlint": "commitlint --from HEAD~1 --to HEAD --verbose",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0",
    "prettier": "^3.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "@angular-eslint/eslint-plugin": {
      "optional": true
    },
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "husky": "^9.1.7",
    "semantic-release": "^25.0.2"
  }
}
